<div class="bg-white full-height">
  <%= form_with model: @user, url: sign_up_path do |form| %>
    <%= div_reg_start%>
    <%= display_reg_errors %>
    <%= div_reg_end%>
    <%= form.label :name, class: "text-white" %>
    <%= form.text_field :name, class: "form-control" %>
    <%= form.submit class: "btn btn-success form-control mt-2" %>
  <% end %>
</div>