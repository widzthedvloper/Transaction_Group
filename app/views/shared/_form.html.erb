<%= form_with model: @user, url: sign_up_path do |form| %>
  <% if @user.errors.any? %>
    <div>
      <% @user.errors.full_messages.each do |message|%>
        <div><%= message %></div>
      <%end%>
    </div>
  <% end %>
  <%= form.label :name%>
  <%= form.text_field :name %>
  <%= form.submit%>
<% end %>