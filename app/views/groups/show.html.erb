<div>
  <h2>Group Transaction</h2>
</div>
<div>
  <p>Total Transaction: <%= @count %></p>
</div>
<% @group_transaction.each do |transactions| %>
  <div>
    <div>
     <%= image_tag('transactions.png') %>
    </div>
    <div>
      <p><%= transactions.name %></p>
      <p><%= transactions.created_at %></p>
    </div>
    <div>
     <p>$<%= transactions.amount %></p>
    </div>
  </div>
<% end %>