<%= form_with model: @groups, url: create_group_path do |form| %>
  <% if @groups.errors.any? %>
    <div>
      <% @groups.errors.full_messages.each do |message|%>
        <div><%= message %></div>
      <%end%>
    </div>
  <% end %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :icon %>
  <%= form.text_field :icon %>
  <%= form.number_field :user_id, value: Current.user.id, type: :hidden %>
  <%= form.submit %>
<% end %>
